
@{
    ViewData["Title"] = "IconUpload";
}

<img src="@ViewBag.imgsrc" />
<form asp-action="IconUpload" method="post" enctype="multipart/form-data">
    <div class="form-group">
       Icon Name: <input type="text" name="txticonname" id="txticonname" />
    </div>
    <div class="form-group">
        <input type="file" name="user_image" id="user_image" />
    </div>
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-default" />
    </div>
</form>
